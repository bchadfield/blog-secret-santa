#csw-content-counter { z-index: 100; float: right; position: relative; top: 10px; right: 10px; font-size: 0.875rem; }
#csw-content-counter.csw-count-over,
.red-alert { color: #dd0000;}
@media (min-width: 768px) {
	.csw-right-column { position: fixed; top: 0; right: 0; padding-top: 7.1875rem; height: 100%; }
	body.csw-content-layout-simple .csw-right-column { padding-top: 4.375rem; }
	body.csw-content-layout-body .csw-content-body-wrapper { margin-top: 6.875rem; }
	body.csw-content-layout-body.csw-content-layout-simple .csw-content-body-wrapper,
	#csw-preview-html-wrapper { margin-top: 4.0625rem; }
	.csw-content-header { z-index: 100; }
	.csw-content-header .csw-content-info { float: left; }
	.csw-content-header .csw-header-center,
	.csw-content-header .csw-content-previewed { position: absolute; }
	/*.csw-content-sub-header { position: fixed; top: 4.0625rem; }*/
	/*.csw-content-sub-header.csw-content-actions { width: 66.67%; }*/
	/*.csw-right-column .csw-content-sub-header { width: 33.34%; right: 0; }*/
	.csw-footer { position: absolute; bottom: 0; }
	.csw-content-plain-preview { padding: 30px 20px 50px; width: 50rem; margin: 0 auto; }
	.csw-content-actions-menu { float: left; width: 100%; }
	.csw-content-sub-header .csw-assigned-users { float: right; max-width: 50%; width: auto; padding: 0; }
