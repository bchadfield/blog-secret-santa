.csw-content-header .csw-content-previewed form .csw-submit:hover { color: #333; background-color: #ebebeb; border-color: #adadad; }
.csw-content-header .csw-back-link { float: left; padding: 1.2rem 0; position: relative; z-index: 100; }
.csw-content-sub-header { border-bottom: 1px solid #e9e9ea; background-color: #fff; font-size: 0.875rem; height: 2.8125rem; }
.csw-content-actions-menu { list-style: none; margin: 0; padding: 0.6rem 0 0.6rem 20px; }
.csw-content-actions-menu > li { position: relative; float: left; margin: 0 0.5rem; line-height: 1.5rem; }
.csw-content-actions-menu > li:first-child { margin-left: 0; }
.csw-content-actions-menu > li > a { color: #666; text-decoration: none; }
.csw-content-actions-menu > li > a:hover,
.csw-content-actions-menu > li > a:active { color: #888; }
.csw-content-actions-menu .csw-icon-chevron-left { top: 2px; }
/*.csw-content-sub-header.csw-content-actions { padding: 0 10px 0 20px; z-index: 2; }*/
.csw-content-actions .csw-content-actions-left,
.csw-content-actions .csw-content-actions-right { width: 50%; float: left; }
.csw-content-actions .csw-content-actions-left {  }
.csw-content-sub-header .csw-assigned-users { width: 100%; position: relative; top: 4px; padding: 15px 0 6px 0; white-space: nowrap; }
.csw-content-sub-header .csw-assigned-users .csw-user { margin-bottom: 0; margin-right: 1px; }
.csw-content-sub-header .csw-assigned-users .csw-user .csw-avatar { margin-bottom: 0; width: 36px; height: 36px; border-radius: 18px; }
.csw-content-right-tabs { margin: 0; padding: 0; list-style: none; }
.csw-content-right-tabs > li { float: left; border-right: 1px solid #f3f3f4; background-color: #fafafa; }
.csw-content-right-tabs > li:first-child { border-left: 1px solid #f3f3f4; }
.csw-content-right-tabs > li > a { display: inline-block; padding: 0.75rem 1rem; color: #888; text-decoration: none; }
.csw-content-right-tabs > li.csw-tab-active { border-bottom: 1px solid #fff; background-color: #fff; }
.csw-content-right-tabs > li.csw-tab-active > a { color: #666; text-decoration: none; }
.csw-content-right-tabs > li:not(.csw-tab-active) > a:hover { color: #666; }
.csw-text-button.csw-font-review,
.csw-text-button.csw-font-review:hover, 
.csw-text-button.csw-font-review:focus { color: #f0ad4e; }
.csw-text-button.csw-font-ready,
.csw-text-button.csw-font-ready:hover, 
.csw-text-button.csw-font-ready:focus { color: #5cb85c; }
.csw-workflow-actions .csw-button .csw-caret { margin-left: 0.3em; }
.csw-workflow-actions .csw-button.csw-parked .csw-caret { border-top-color: #888; }
.csw-modal-body.csw-send-assignments p { margin-bottom: 0.5rem; }
.csw-content-plain-preview { padding: 0 20px; }
.csw-content-types,
.csw-activities,
.csw-versions { margin-top: 15px; }
.csw-content-body { margin-top: 20px; }
.csw-content-types .csw-form-group:last-child { margin-bottom: 0; }
.csw-right-column { font-size: 0.875em; }
.csw-comment-form { margin-top: 15px; padding: 23px 0 15px 10px; }
#content_title { border: none; padding: 0; font-size: 2rem; font-weight: 500; line-height: 1.5em; margin-bottom: 1rem; }
body.csw-content-layout-simple .csw-comment-form { margin-top: 5px; }
.csw-comment-form .csw-form-input { padding: 0.375rem; line-height: 1.125rem; }
.csw-comments .csw-scroll-viewport { overflow-y: scroll; position: relative; }
.csw-modal img.help-screenshot { width: 100%; margin-bottom: 1.25rem; }
.csw-modal-body p { font-family: inherit; font-weight: normal; font-size: 1rem; line-height: 1.6; margin: 0 0 1.25rem; text-rendering: optimizeLegibility; }
.csw-footer { width: 100%; }
.csw-footer .csw-footer-links { margin: 10px 0; font-size: 0.9em; text-align: center; color: #ccc }
.csw-footer .csw-footer-links a { color: #999; }
.csw-footer .csw-footer-links a:hover { color: #2a6496; }
.csw-content-layout-simple .csw-content-column { padding: 0; margin-left: -20px; margin-right: -10px; }
#csw-left-nav { font-size: 0.875rem; left: -100px; }
html.csw-left-nav-visible, body.csw-left-nav-visible { overflow-x: hidden; overflow-y: scroll; width: 100%; height: 100%; }
.csw-main-wrapper, .csw-content-header, #csw-left-nav {
  -webkit-transition: -webkit-transform 0.1s linear;
  -moz-transition: -moz-transform 0.1s linear;
  -o-transition: -ms-transform 0.1s linear;
  -ms-transition: -o-transform 0.1s linear;
  transition: transform 0.1s linear;
}
.csw-main-wrapper { background-color: #fff; z-index: 3; }
